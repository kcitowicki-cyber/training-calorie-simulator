training_minutes = 60
calories_minute = 7
total_calories = training_minutes * calories_minute
calories_per_week = 3 * total_calories
calories_per_month = 4 * calories_per_week

print("Czas treningu:", training_minutes)
print("Spalone kalorie:", total_calories)
print("Spalone kalorie przy założeniu trzech 60 minutowych treningów w tygodniu:", calories_per_week)
print("spaline kalorie przy założeniu dwunastu 60 minutowych treningów w miesiacu:", calories_per_month)
